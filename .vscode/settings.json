{
  "cSpell.enableFiletypes": ["vue"],
  "cSpell.words": [
    "ASOUL",
    "cnki",
    "FANMADE",
    "toastification",
    "Uploaders",
    "Vite"
  ],
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "eslint.validate": ["javascript", "javascriptreact", "vue"],
